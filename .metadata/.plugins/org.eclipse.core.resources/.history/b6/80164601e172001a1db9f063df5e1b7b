package p5729;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner cin = new Scanner(System.in);
		int w = cin.nextInt();
		int x = cin.nextInt();
		int h = cin.nextInt();
		int q = cin.nextInt();
		//思路，切时做判断，未切则切，且作标记以及cuted已切数+1,最后用体积减去cuted可得结果
		//这样会省时间，不用最后做一个三次循环
		int cuted = 0;//被切了的方块数
		int squre[][][] = new int[w][x][h];  
		for(int i=0;i<q;i++){
			int x1 = cin.nextInt();
			int y1 = cin.nextInt();
			int z1 = cin.nextInt();
			int x2 = cin.nextInt();
			int y2 = cin.nextInt();
			int z2 = cin.nextInt();
			for(;x1<=x2;x1++){
				for(;y1<=y2;y1++){
					for(;z1<=z2;z1++){
						if(squre[x1][y1][z1]==0){
							squre[x1][y1][z1] = 1;
							cuted++;
						}
					}
				}
			}
		}
		System.out.println(w*x*h-cuted);
		
	}

}
