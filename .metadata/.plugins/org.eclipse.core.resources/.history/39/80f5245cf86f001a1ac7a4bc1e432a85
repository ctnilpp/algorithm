package p4956;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner cin =  new Scanner(System.in);
		int n =cin.nextInt();
		int x =0;
		int k =1;
		int sum =0;
		int when =1;//(1,2,3,4,5,6,7)分别是星期几
		int day =1;//第几天
		for(x=100;x>=1;x--){
			day=1;
			when=1;
			sum=0;
			while(sum<=n&&day<=52){
				sum+=x+when*k;
				day++;
			}
			if(sum==n){
				System.out.println(x);
				System.out.println(k);
				break;
			}
		}
	}

}
