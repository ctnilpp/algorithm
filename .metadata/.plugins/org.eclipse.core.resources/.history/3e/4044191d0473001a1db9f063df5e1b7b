package p5730;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner cin = new Scanner(System.in);
		char a[][][] = new char[10][5][3];
		char [][]a0={
				{'X','X','X'},
				{'X','.','X'},
				{'X','.','X'},
				{'X','.','X'},
				{'X','X','X'}
				};
		a[0] = a0;
		for(int i=0;i<5;i++){
			for(int j=0;j<3;j++){
				System.out.print(a[0][i][j]);
			}
			System.out.println();
		}
	}

}
