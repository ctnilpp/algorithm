package p5734;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner cin = new Scanner(System.in);
		int n = cin.nextInt();
		String old = cin.next();
		for(int i=0;i<n;i++){
			String now = cin.next();
			int operation = (int)now.charAt(0)-48;
			System.out.println(operation);
			StringBuffer tnow = new StringBuffer(old);
			System.out.println("tnow:"+tnow.toString());
			switch(operation){
			case 1:
				String addstr = cin.next();
				System.out.println(addstr);
				System.out.println("length:"+now.length());
				for(int j=2;j<now.length();j++){
					System.out.println(now.charAt(j));
//					tnow.append(now.charAt(j));
				}
//				System.out.println(tnow.toString());
				break;
			case 2:
				break;
			case 3:
				break;
			case 4:
				break;
			default:
			}
		}
		
	}

}
