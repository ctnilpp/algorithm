package p5706;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner cin = new Scanner(System.in);
		double t = cin.nextDouble();
		int num = cin.nextInt();
		System.out.println(t/num);
		System.out.println(num*2);
	}

}
