package p5720;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner cin = new Scanner(System.in);
		double a = cin.nextDouble();
		int day =1;
		while(a>1){
			day+=1;
			a = Math.floor(a/2);
		}
		System.out.println(day);
	}

}
