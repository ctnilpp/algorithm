package p4956;

import java.util.Scanner;

public class Main {
	static int n=0;
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner cin =  new Scanner(System.in);
		n =cin.nextInt();
		int x =0;
		int sum=0;
		int k =1;
		int day=1;
		int sumday=1;
		for(x=100;x>=1;){
			while(sum<n&&sumday<=52){
				sum+=x+day*k;
				System.out.println(sum);
				if(day==7)day=1;
				else day++;
				sumday++;
			}
			if(sum<n)k++;
			if(sum==n&&sumday==52){
				System.out.println(x);
				System.out.println(k);
				break;
			}
			else {
				day=1;
				sumday=1;
				x--;
			}
		}
	}

	

}
