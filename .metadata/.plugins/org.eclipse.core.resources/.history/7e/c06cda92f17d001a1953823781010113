package p5015;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner cin = new Scanner(System.in);
		System.out.println((int)'a'+" "+(int)'z'+" "+(int)'0'+" "+(int)'9'+" "+(int)'A'+" "+(int)'Z'+" ");
		String now = cin.next();
		int delete = 0;
		for(int i=0;i<now.length();i++){
			if((i>=97&&i<=122)||(i>=48&&i<=57)||(i>=65&&i<=90)){
				delete++;
			}
		}
		System.out.println(now.length()-delete);
	}

}
